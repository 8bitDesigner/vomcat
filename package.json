{
  "name": "vomcat",
  "version": "1.0.0",
  "description": "Record when cat vom",
  "main": "index.js",
  "scripts": {
    "test": "mocha --require babel/register --recursive test",
    "watch": "mocha --require babel/register --recursive -w test",
    "develop": "supervisor -e 'js,ejs,jsx' index.js",
    "build": "browserify -r ./app/public/jsx/app.jsx:App -o app/public/js/app.js",
    "start": "node ./index.js"
  },
  "author": "Paul Sweeney <paul@8-bitdesign.com>",
  "license": "ISC",
  "dependencies": {
    "babel": "^4.5.5",
    "bootstrap": "^3.3.2",
    "browser-request": "^0.3.3",
    "browserify": "^8.1.3",
    "ejs": "^2.2.4",
    "express": "^4.11.2",
    "redis": "^0.12.1"
  },
  "devDependencies": {
    "mocha": "^2.1.0",
    "dotenv": "^0.5.1",
    "moment": "^2.9.0",
    "watchify": "^2.4.0",
    "react": "^0.13.0",
    "reactify": "^1.0.0"
  },
  "engines": {
    "node": "0.10.x"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:8bitDesigner/vomcat.git"
  },
  "browser": "public/jsx/app.jsx",
  "browserify": {
    "transform": [
      [
        "reactify",
        {
          "es6": true,
          "target": "es5"
        }
      ]
    ]
  }
}
